import turtle

turtle.pensize(5)
turtle.speed(0)
def halfheart():
    for i in range(200):
        turtle.right(1)
        turtle.forward(1)

turtle.speed(0)
turtle.color("red","red")
turtle.begin_fill()
turtle.left(140)
turtle.forward(111.65)
halfheart()

turtle.left(120)
halfheart()
turtle.forward(111.65)
turtle.end_fill()
